{
  "privateKeyPath": "~/.ssh/id_rsa",
  "passphrase": true,
  "sshConfigPath": "~/.ssh/config",
  "uploadOnSave": false,
  "context": "dist",
  "syncMode": "update",
  "watcher": {
    "files": "**/*",
    "autoUpload": false,
    "autoDelete": false
  },
  "ignore": ["**/.vscode/**", "**/.git/**", "**/.DS_Store"],
  "profiles": {
    "dev": {
      "protocol": "sftp",
      "username": "",
      "host": "",
      "port": 22,
      "remotePath": "/var/www/html/"
    }
  },
  "defaultProfile": "dev"
}
